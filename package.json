{
  "name": "simple-ts-ecs-simulation",
  "version": "0.1.15",
  "description": "A simple ECS based Simulation framework",
  "keywords": [
    "typescript",
    "ECS",
    "simulation"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rm -rf ./dist/",
    "build": "tsc",
    "clean": "rm -rf ./dist/",
