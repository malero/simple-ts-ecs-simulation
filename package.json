  },
  "devDependencies": {
    "@types/jasmine": "^3.6.10",
    "codecov": "^3.8.1",
    "jasmine": "^3.7.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.1",
    "typescript": "^4.2.4"
  },
  "homepage": "https://github.com/malero/simple-ts-ecs-simulation",
  "git": "https://github.com/malero/simple-ts-ecs-simulation",
  "typings": "./dist/index.d.ts",
